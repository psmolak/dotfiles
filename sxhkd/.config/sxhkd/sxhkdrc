super + Return
	urxvt

super + space
	dmenu_run

super + u
	selsave; \
	twmnc --content "Saved in clipboard!" --pos top_center

super + shift + u
	sel-fzf

{super + grave,Menu}
	tylda

super + shift + r
	pkill -USR1 -x sxhkd

alt + w
	hsetroot -fill $(find -L ~/wallpapers -type f | shuf -n1)

XF86Audio{Lower,Raise}Volume
	volume {-,+} 5% && twmnc -t 'Volume' -c "$(volume level)" --id 100

XF86AudioMute
	volume '!' && twmnc -t 'Volume' -c "$(volume state)" --id 100

